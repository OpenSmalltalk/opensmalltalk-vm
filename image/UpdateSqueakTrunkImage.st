[[[MCMcmUpdater			defaultUpdateURL: 'http://source.squeak.org/trunk';			updateFromServer] valueSupplyingAnswer: true]		on: Deprecation		do: [:ex| ex resume: nil]]	on: Warning	do: [:ex| | text |		text := ex messageText.		((text beginsWith: 'This package depends on the following classes')		 or: [text beginsWith: 'About to serialize an empty diffy version.']) ifFalse:			[ex pass].		ex resume].Smalltalk snapshot: true andQuit: true